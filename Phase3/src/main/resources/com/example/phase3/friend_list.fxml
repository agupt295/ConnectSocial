<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="mainAnchorPane" prefHeight="900.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.phase3.FriendList_controller">
   <children>
      <VBox layoutX="1.0" prefHeight="1000.0" prefWidth="900.0">
         <children>
            <HBox prefHeight="64.0" prefWidth="321.0">
               <children>
                  <ToolBar prefHeight="50.0" prefWidth="900.0">
                    <items>
                      <Button fx:id="back" mnemonicParsing="false" onAction="#backToProfile" text="Back" />
                    </items>
                  </ToolBar>
               </children>
            </HBox>
            <Button fx:id="viewProfile" mnemonicParsing="false" onAction="#viewFriendProfile" prefHeight="32.0" prefWidth="101.0" text="View Profile" />
            <Label fx:id="warning" prefHeight="27.0" prefWidth="901.0" />
            <Label fx:id="friendCount" prefHeight="42.0" prefWidth="160.0" text="Friends' List">
               <font>
                  <Font size="16.0" />
               </font></Label>
            <ListView fx:id="friendList" prefHeight="818.0" prefWidth="900.0" />
         </children>
      </VBox>
   </children>
</AnchorPane>
